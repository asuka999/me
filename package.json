{
  "name": "me",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "serve": "npm run run",
    "run": "nodemon -w dist/ --ignore dist/public dist/main.js ",
    "clean": "rm -rf ./dist",
    "watch": "copy-file & npm run babel & npm run webpack-client & npm run webpack-server",
    "copy-file": "cp src/view/index.html dist/view/index.html && cp src/view/favicon.ico dist/public/favicon.ico",
    "postinstall": "ls ./node_modules/.bin && ./node_modules/.bin/babel src -d dist & ./node_modules/.bin/webpack --config ./build/webpack.server.config.js & ./node_modules/.bin/webpack --config ./build/webpack.client.config.js && npm run copy-file",
    "webpack-client": "webpack --watch --config ./build/webpack.client.config.js",
    "webpack-server": "webpack --watch --config ./build/webpack.server.config.js",
    "babel": "babel --watch src -d dist --source-maps",
    "debug": "node-debug --hidden node_modules/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "echo help",
    "docs": "echo docs"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-vue": "^1.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "ignore-loader": "^0.1.1",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.5.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.25"
  },
  "dependencies": {
    "github-markdown-css": "^2.4.1",
    "humps": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-session": "^3.3.1",
    "koa-static": "^3.0.0",
    "lru-cache": "^4.0.1",
    "marked": "^0.3.6",
    "mongodb": "^2.2.10",
    "normalize.css": "^4.2.0",
    "qiniu": "^6.1.11",
    "serialize-javascript": "^1.3.0",
    "vue": "^2.0.1",
    "vue-router": "^2.0.0",
    "vue-server-renderer": "^2.0.1",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  }
}
